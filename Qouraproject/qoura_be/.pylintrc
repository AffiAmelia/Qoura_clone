[BASIC]
max-line-length = 120

[FORMAT]
indent-string = "    "

[TYPECHECK]
ignored-modules = collections.defaultdict

[DESIGN]
max-args = 6

[SIMILARITIES]
min-similarity-lines = 5

[MESSAGES CONTROL]
disable = C0114, C0115, C0116 ,C0411,C0412, missing-function-docstring , unsupported-binary-operation ,  too-few-public-methods ,
 no-member, too-many-ancestors, W0613, E0307s , W0012 , E1101,C0103
